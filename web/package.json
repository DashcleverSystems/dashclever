{
  "name": "web-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "build": "npm run write:env -s && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "write:env": "ngx-scripts env api_url --format json",
    "lint": "ng lint",
    "generate:api": "openapi-generator-cli generate -g typescript-angular -i ../build/openapi.json -o ./generated/openapi --skip-validate-spec",
    "clean:generate:api": "rm -rf generated/openapi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.1",
    "@angular/common": "^17.2.1",
    "@angular/compiler": "^17.2.1",
    "@angular/core": "^17.2.1",
    "@angular/forms": "^17.2.1",
    "@angular/localize": "^17.0.2",
    "@angular/platform-browser": "^17.2.1",
    "@angular/platform-browser-dynamic": "^17.2.1",
    "@angular/router": "^17.2.1",
    "@angular/service-worker": "^17.2.1",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngrx/component-store": "^17.1.0",
    "@ngrx/effects": "^17.1.0",
    "@ngrx/store": "^17.1.0",
    "@ngrx/store-devtools": "^17.1.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "angular2-uuid": "^1.1.1",
    "chart.js": "^4.4.1",
    "fastdom": "^1.0.11",
    "https-proxy-agent": "^7.0.3",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "rxjs": "~7.8.0",
    "stompjs": "^2.3.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tslint": "^6.1.3",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "~17.2.0",
    "@angular/compiler-cli": "^17.2.1",
    "@angular/language-service": "^17.2.1",
    "@ngx-rocket/scripts": "^5.2.3",
    "@types/jasmine": "~5.1.4",
    "@types/lodash": "^4.14.202",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.11.18",
    "@types/object-hash": "^3.0.6",
    "@types/prettier": "2.7.3",
    "@types/stompjs": "^2.3.9",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.33.2",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.2.5",
    "primeng": "^16.4.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.3.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
