{
  "name": "web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "prestart": "npm run generate:api",
    "build": "npm run write:env -s && ng build",
    "prebuild": "npm run generate:api",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "write:env": "ngx-scripts env npm_package_version",
    "lint": "ng lint",
    "generate:api": "openapi-generator-cli generate -g typescript-angular -i ../build/openapi.json -o ./generated/openapi --skip-validate-spec",
    "pregenerate:api": "rm -rf generated/openapi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/localize": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.2.0",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngrx/component-store": "^16.2.0",
    "@ngrx/effects": "^16.2.0",
    "@ngrx/store": "^16.2.0",
    "@ngrx/store-devtools": "^16.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "https-proxy-agent": "^7.0.1",
    "lodash": "^4.17.21",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.1.1",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "16.1.1",
    "@angular-eslint/schematics": "16.1.1",
    "@angular-eslint/template-parser": "16.1.1",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/language-service": "^16.2.3",
    "@ngx-rocket/scripts": "^5.2.3",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.33.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "primeng": "^16.2.3",
    "typescript": "~5.1.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
